#define TARGET_OSX
#define IX86
#define PIC

PLATFORMLIB = osx_x86
SVNDIR = osx_x86
SDK = /Developer/SDKs/MacOSX10.4u.sdk

CCFLAGS=%(CCFLAGS) -arch i386 -fPIC -mmacosx-version-min=10.4
ASMFLAGS = -f macho32 -D_MACHO -D_PIC -D_HIDDEN
NASMFLAGS = -f macho32 -D_MACHO -D_PIC -D_HIDDEN

#include "gcc_osx.inc"

